{
	"env": {
		"worker": true,
		"jest/globals": true
	},
	"extends": "airbnb-base",
	"globals": {
		"TOKEN": "readonly"
	},
	"plugins": [
		"jest"
	],
	"rules": {
    "arrow-body-style": "off",
    "comma-dangle": "off",
    "no-console": "warn",
    "no-param-reassign": "off",
    "no-restricted-globals": "off",
    "no-unused-vars": "off",
    "quote-props": ["error", "consistent"],
    "quotes": ["error", "single"]
  }
}
